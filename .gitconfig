[alias]
        di = diff --cached
        pa = push --all
        co = checkout
        st = status
	ci = commit
        push-it-real-good = push -f
[color]
	interactive = auto
	diff = auto
[core]
	fsyncobjectfiles = true
	excludesfile = /Users/denbuen/.gitignore_global
[difftool "sourcetree"]
	cmd = opendiff \"$LOCAL\" \"$REMOTE\"
	path = 
[push]
	default = simple
[guitool "stash/keeping index"]
	cmd = git stash -k -u
	noconsole = yes
[guitool "stash/pop"]
	cmd = git stash pop
	noconsole = yes
[guitool "delete file"]
	cmd = rm \"$FILENAME\"
	needsfile = yes
        confirm = yes
[diff]
	tool = vimdiff
[merge]
	tool = vimdiff
[difftool]
	prompt = false
[difftool "vimdiff"]
	path = /Applications/MacVim.app/Contents/bin/vimdiff
